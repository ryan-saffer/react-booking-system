{
    "name": "booking-system",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build-fizz-kidz": "npm --prefix ../server/fizz-kidz run build",
        "build-fizz-kidz:watch": "npm --prefix ../server/fizz-kidz run build:watch",
        "start": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"vite --mode dev\"",
        "start:prod": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"vite --mode prod\"",
        "build:dev": "npm run lint && npm run build-fizz-kidz && npm run ts:check && vite build --mode dev",
        "build:prod": "npm run lint && npm run build-fizz-kidz && npm run ts:check && vite build --mode prod",
        "preview": "vite preview",
        "ts:check": "tsc -b",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
        "test": "vitest --run",
        "test:coverage": "vitest run --coverage",
        "eject": "react-scripts eject"
    },
    "dependencies": {
