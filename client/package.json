{
    "name": "booking-system",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build-fizz-kidz": "npm --prefix ../server/fizz-kidz run build",
        "build-fizz-kidz:watch": "npm --prefix ../server/fizz-kidz run build:watch",
        "vite": "VITE_ENV=dev vite",
        "start": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"VITE_ENV=dev vite\"",
        "start:prod": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"VITE_ENV=prod vite\"",
        "build:dev": "npm run lint && npm run build-fizz-kidz && npm run ts:check && VITE_ENV=dev vite build",
        "build:prod": "npm run lint && npm run build-fizz-kidz && npm run ts:check && VITE_ENV=prod vite build",
        "preview": "vite preview",
        "ts:check": "tsc -b",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@ant-design/icons": "^5.2.6",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
