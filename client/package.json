{
    "name": "booking-system",
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build-fizz-kidz": "npm --prefix ../server/fizz-kidz run build",
        "build-fizz-kidz:watch": "npm --prefix ../server/fizz-kidz run build:watch",
        "vite": "VITE_ENV=dev vite --host",
        "start": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"VITE_ENV=dev vite --host\"",
        "start:prod": "concurrently \"npm run build-fizz-kidz:watch\" \"npm run ts:check -- --watch\" \"VITE_ENV=prod vite\"",
        "build:dev": "npm run lint && npm run build-fizz-kidz && npm run ts:check && VITE_ENV=dev vite build",
        "build:prod": "npm run lint && npm run build-fizz-kidz && npm run ts:check && VITE_ENV=prod vite build",
        "preview": "vite preview",
        "ts:check": "tsc -b",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@ant-design/icons": "^5.2.6",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.14",
        "@mui/material": "^5.14.14",
        "@mui/x-date-pickers": "^6.16.3",
        "@stripe/react-stripe-js": "^1.9.0",
        "@stripe/stripe-js": "^1.32.0",
        "@tanstack/react-query": "^4.36.1",
        "@trpc/client": "^10.43.3",
        "@trpc/react-query": "^10.43.3",
        "@trpc/server": "^10.43.3",
        "antd": "^5.4.0",
        "autoprefixer": "9.8.0",
        "dateformat": "^3.0.3",
        "firebase": "^10.6.0",
        "fizz-kidz": "file:../server/fizz-kidz",
        "googleapis": "^39.2.0",
        "luxon": "^3.4.3",
        "mixpanel-browser": "^2.45.0",
        "moment-timezone": "^0.5.28",
        "query-string": "^6.8.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-markdown": "^9.0.0",
        "react-router-dom": "^6.11.2",
        "react-signature-canvas": "^1.0.3",
        "typeface-roboto": "^1.1.13"
    },
    "devDependencies": {
        "@types/dateformat": "^3.0.1",
        "@types/luxon": "^2.3.2",
        "@types/mixpanel-browser": "^2.38.0",
        "@types/node": "^14.14.37",
        "@types/react": "^18.2.33",
        "@types/react-dom": "^18.2.14",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "concurrently": "^8.2.1",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^8.4.1",
        "typescript": "^5.0.4",
        "vite": "^4.4.5"
    },
    "importSort": {
        ".ts, .tsx, .js, .jsx": {
            "style": "import-sort-style-module-scoped"
        }
    }
}
