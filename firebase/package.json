{
    "name": "booking-system",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "build-fizz-kidz": "npm --prefix functions/fizz-kidz run build",
        "build-fizz-kidz:watch": "npm --prefix functions/fizz-kidz run build:watch",
        "start": "concurrently \"npm run build-fizz-kidz:watch\" \"REACT_APP_ENV=dev BROWSER=none react-scripts start\"",
        "start:prod": "npm run build-fizz-kidz:watch | REACT_APP_ENV=prod BROWSER=none react-scripts start",
        "build:prod": "npm run build-fizz-kidz && REACT_APP_ENV=prod react-scripts build",
        "build:dev": "npm run build-fizz-kidz && REACT_APP_ENV=dev react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@date-io/date-fns": "^1.3.11",
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.3",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/pickers": "^3.2.10",
        "@stripe/react-stripe-js": "^1.9.0",
        "@stripe/stripe-js": "^1.32.0",
        "@types/node": "^14.14.37",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "antd": "^5.4.0",
        "autoprefixer": "9.8.0",
        "date-fns": "^2.14.0",
        "dateformat": "^3.0.3",
        "firebase": "^9.10.0",
        "fizz-kidz": "file:functions/fizz-kidz",
        "googleapis": "^39.2.0",
        "luxon": "^2.4.0",
        "mixpanel-browser": "^2.45.0",
        "moment-timezone": "^0.5.28",
        "query-string": "^6.8.3",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-hook-form": "^7.43.9",
        "react-router-dom": "^6.11.2",
        "react-scripts": "^5.0.1",
        "react-signature-canvas": "^1.0.3",
        "recompose": "^0.30.0",
        "typeface-roboto": "^1.1.13"
    },
    "devDependencies": {
        "@types/dateformat": "^3.0.1",
        "@types/luxon": "^2.3.2",
        "@types/mixpanel-browser": "^2.38.0",
        "@types/recompose": "^0.30.7",
        "concurrently": "^8.2.1",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^8.4.1",
        "typescript": "^5.0.4"
    },
    "overrides": {
        "typescript": "^5.0.4"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
