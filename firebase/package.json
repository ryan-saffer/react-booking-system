{
  "name": "booking-system",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build-fizz-kidz": "npm --prefix functions/fizz-kidz run build",
    "build-fizz-kidz:watch": "npm --prefix functions/fizz-kidz run build:watch",
    "start": "npm run build-fizz-kidz:watch | REACT_APP_ENV=dev craco start",
    "start:prod": "npm run build-fizz-kidz:watch | REACT_APP_ENV=prod craco start",
    "build-prod": "npm run build-fizz-kidz && REACT_APP_ENV=prod craco build",
    "build-dev": "npm run build-fizz-kidz && REACT_APP_ENV=dev craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^6.4.5",
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@stripe/react-stripe-js": "^1.9.0",
    "@stripe/stripe-js": "^1.32.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "antd": "^4.21.4",
    "autoprefixer": "9.8.0",
    "craco-antd": "^2.0.0",
    "date-fns": "^2.14.0",
    "dateformat": "^3.0.3",
    "firebase": "^9.10.0",
    "fizz-kidz": "file:functions/fizz-kidz",
    "googleapis": "^39.2.0",
    "luxon": "^2.5.2",
    "mixpanel-browser": "^2.45.0",
    "moment-timezone": "^0.5.28",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1",
    "react-signature-canvas": "^1.0.3",
    "recompose": "^0.30.0",
    "typeface-roboto": "0.0.75",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/luxon": "^2.3.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/recompose": "^0.30.7"
